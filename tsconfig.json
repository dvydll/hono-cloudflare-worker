{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"allowJs": true,
		"checkJs": true,
		"sourceMap": true,
		"lib": ["ESNext", "DOM"],
		"types": ["@cloudflare/workers-types/2023-07-01"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"baseUrl": ".",
		"paths": {
			"#/*": ["src/*"]
		}
	},
	"include": ["src/**/*"],
	"exclude": [
		"node_modules",
		"dist",
		"build",
		"scripts",
		"src/**/*.spec.ts",
		"src/**/*.js"
	]
}
